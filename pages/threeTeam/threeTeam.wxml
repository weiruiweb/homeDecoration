<!--user.wxml-->

<view class="User_cont">
	<block wx:for="{{web_mainData}}" wx:key="id">
		<view class="clientList flexRowBetween">
			<image src="{{item.headImgUrl}}" style="border-radius:50%"></image>
			<view class="clientList_center" style="align-items:center;">
				<view class="clientName">
					<text style="line-height:63px">{{item.nickname}}</text>
				</view>
			</view>
			<view class="clientList_right">
				<view class="clientTime">{{item.create_time}}</view>
			</view>
		</view>
	</block>
</view>

